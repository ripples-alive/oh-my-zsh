# Path to oh-my-zsh installation.
export ZSH=$HOME/.oh-my-zsh

# Add wisely, as too many plugins slow down shell startup.
plugins=(autojump command-not-found docker extract git httpie sudo)
plugins+=(copydir copyfile cp)
plugins+=(pip adb)
plugins+=(colored-man-pages colorize)
# plugins+=(jsontools urltools)
# plugins+=(systemd)
# plugins+=(osx brew jenv)

# Enable oh-my-zsh.
source $ZSH/oh-my-zsh.sh
